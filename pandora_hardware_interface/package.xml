<?xml version="1.0"?>
<package format="2">
  <name>pandora_hardware_interface</name>
  <version>0.0.0</version>
  <description>Contains pandora controller packages used to interface with hardware.</description>

  <maintainer email="gkourosg@yahoo.gr">George Kouros</maintainer>

  <license>BSD</license>

  <url type="bugtracker">https://github.com/pandora-auth-ros-pkg/pandora_hardware_interface/issues</url>
  <url type="repository">https://github.com/pandora-auth-ros-pkg/pandora_hardware_interface</url>

  <author>Evangelos Apostolidis</author>
  <author>Chris Zalidis</author>

  <buildtool_depend>catkin</buildtool_depend>

  <exec_depend>pandora_dynamixel_hardware_interface</exec_depend>
  <exec_depend>pandora_imu_hardware_interface</exec_depend>
  <exec_depend>pandora_motor_hardware_interface</exec_depend>
  <exec_depend>pandora_xmega_hardware_interface</exec_depend>
  <exec_depend>pandora_arm_hardware_interface</exec_depend>
  <exec_depend>pandora_linear_actuator_hardware_interface</exec_depend>
  <exec_depend>pandora_hardware_tools</exec_depend>
  <exec_depend>pandora_sensor_msgs</exec_depend>
  <exec_depend>pandora_leddar_hardware_interface</exec_depend>
  <exec_depend>pandora_monstertruck_hardware_interface</exec_depend>

  <export>
    <metapackage/>
  </export>
</package>
